{
  "name": "mikayla-fun-app",
  "version": "1.0.0",
  "description": "Electron app with sidebar and browser tab",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:tsc\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:tsc": "concurrently \"tsc -p tsconfig.node.json --watch\" \"node scripts/watch-preload.js\"",
    "dev:electron": "wait-on dist/main.js dist/preload.cjs http://localhost:5173 && electron .",
    "build": "npm run build:tsc && npm run build:vite",
    "build:tsc": "tsc -p tsconfig.node.json && tsc -p tsconfig.preload.json && node scripts/rename-preload.js",
    "build:vite": "vite build",
    "start": "electron .",
    "clean": "rimraf dist"
  },
  "keywords": [
    "electron",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "postcss": "^8.4.35",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@google/genai": "^1.29.1",
    "@supabase/supabase-js": "^2.57.4",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.344.0",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.6",
    "swiper": "^12.0.3"
  }
}
